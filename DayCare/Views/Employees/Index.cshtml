@model IEnumerable<DayCare.Models.Employee>

@{
    ViewBag.Title = "Index";
}

<h2>All employees</h2>

<p>
    @Html.ActionLink("Add another Employee", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employee_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employee_contact)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Employee_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Employee_Email)
        </td>
  
        <td>
            @Html.DisplayFor(modelItem => item.Employee_contact)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Employee_Id }) |
            @Html.ActionLink("Archive", "Delete", new { id=item.Employee_Id })
        </td>
    </tr>
}

</table>
